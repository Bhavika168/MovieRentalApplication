{
    "name": "@extjs/ext-classic-touch-sizing",
    "ext-react-name": "@extjs/ext-react-classic-touch-sizing",
    "ext-name": "@extjs/ext-classic-touch-sizing",
    "SenchaExtName": "@sencha/ext-classic-touch-sizing",
    "SenchaExtReactName": "@sencha/ext-react-classic-touch-sizing",
    "version": "7.0.0.168",
    "sencha": {
        "name": "touch-sizing",
        "type": "code",
        "alternateName": [
            "ext-touch-sizing"
        ],
        "framework": "ext",
        "toolkit": "classic",
        "creator": "Sencha",
        "summary": "Adds Touch Sizing to a Theme",
        "detailedDescription": "This package sets all the required sass variables to make a theme touch-friendly.",
        "version": "7.0.0.168",
        "compatVersion": "7.0.0",
        "format": "1",
        "output": "${workspace.subpkg.prefix}/${package.name}",
        "local": true,
        "sass": {
            "namespace": "Ext",
            "etc": "${package.dir}/sass/etc/all.scss",
            "var": "${package.dir}/sass/var",
            "src": "${package.dir}/sass/src"
        },
        "classpath": "${package.dir}/src",
        "overrides": "${package.dir}/overrides",
        "slicer": {
            "js": []
        }
    }
}
