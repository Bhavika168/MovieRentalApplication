{
    "name": "@extjs/ext-modern-theme-ios",
    "ext-react-name": "@extjs/ext-react-modern-theme-ios",
    "ext-name": "@extjs/ext-modern-theme-ios",
    "SenchaExtName": "@sencha/ext-modern-theme-ios",
    "SenchaExtReactName": "@sencha/ext-react-modern-theme-ios",
    "version": "7.0.0.168",
    "sencha": {
        "name": "theme-ios",
        "namespace": "Ext",
        "type": "theme",
        "extend": "theme-triton",
        "toolkit": "modern",
        "creator": "Sencha",
        "summary": "Ext JS iOS Theme",
        "detailedDescription": "A modern-toolkit theme that emulates the native look and feel of iOS",
        "version": "7.0.0.168",
        "compatVersion": "7.0.0",
        "format": "1",
        "output": "${workspace.subpkg.prefix}/${package.name}",
        "local": true,
        "sass": {
            "namespace": "Ext",
            "etc": "${package.dir}/sass/etc/all.scss",
            "var": "${package.dir}/sass/var",
            "src": "${package.dir}/sass/src"
        },
        "classpath": "${package.dir}/src",
        "overrides": "${package.dir}/overrides",
        "framework": "ext",
        "requires": [
            "font-ios"
        ],
        "properties": {
            "skip.pkg": 1
        }
    }
}
